{
  "tasks": {
    "dev": "deno serve -WRE --env-file --watch main.ts"
  },
  "imports": {
    "@bepalo/jwt": "jsr:@bepalo/jwt@^2.0.9",
    "@gdquest/type-fest": "jsr:@gdquest/type-fest@^4.41.0",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/http": "jsr:@std/http@^1.0.20",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "valibot": "jsr:@valibot/valibot@^1.0.0"
  },
  "fmt": {
    "singleQuote": true
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "include": [
        "explicit-function-return-type",
        "no-console"
      ]
    }
  },
  "unstable": [
    "kv"
  ]
}